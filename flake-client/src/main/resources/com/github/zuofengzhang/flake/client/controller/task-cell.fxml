<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane xmlns="http://javafx.com/javafx/8.0.281" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.zuofengzhang.flake.client.controller.TaskCellController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="111.0" minWidth="17.0" prefWidth="31.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="0.0" />
        <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" maxWidth="600.0" minWidth="265.0" prefWidth="320.0" />
        <ColumnConstraints halignment="LEFT" hgrow="NEVER" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="127.0" minHeight="4.0" prefHeight="25.0" vgrow="SOMETIMES" />
        <RowConstraints />
    </rowConstraints>
    <children>
        <Label fx:id="titleLabel" maxWidth="1.7976931348623157E308" minWidth="260.0" text="Title " textFill="#000000e2" wrapText="true" GridPane.columnIndex="2">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <CheckBox fx:id="checkBox" maxWidth="1.7976931348623157E308" mnemonicParsing="false" GridPane.halignment="CENTER" GridPane.rowSpan="2" GridPane.valignment="CENTER" />
        <ImageView fx:id="iuaImageView" fitHeight="31.0" fitWidth="31.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="3" />
      <Label fx:id="tagLabel" minWidth="1.0" text="L" textFill="#e10b0b" GridPane.columnIndex="1">
         <font>
            <Font size="16.0" />
         </font></Label>
    </children>
</GridPane>
